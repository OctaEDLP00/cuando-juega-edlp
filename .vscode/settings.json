{
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
  "editor.detectIndentation": false,
  "editor.insertSpaces": false,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "astro",
    "typescript",
    "typescripreact"
  ],
  "files.associations": {
    "*.mdx": "markdown"
	},
	"javascript.format.insertSpaceBeforeFunctionParenthesis": true
}
