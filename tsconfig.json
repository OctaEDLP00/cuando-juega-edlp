{
	"extends": "astro/tsconfigs/strictest",
	"include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": ".",
    "paths": {
      "~/*": [
        "src/*"
      ]
		}
  }
}
